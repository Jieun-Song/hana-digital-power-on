<mapper namespace="Board"> 
<!-- 스프링 MVC게시판 저장 -->	
<insert id = "board_in" parameterType = "net.daum.vo.BoardVO">
insert into tbl_board (bno, writer, title, content, regdate) values(bno_seq.nextval, #{writer),#{title},#{content},sysdate)
</insert>
</mapper>